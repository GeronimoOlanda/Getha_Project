@model Getha_Project.WebSite.Models.Home.GraficoModel;
@{
    ViewData["Title"] = "Getha Desenvolvimento e Consultoria Contabil";
}

<script>
	var Nome_Grafico = "Gráfico Pizza";
	var TipoGrafico = "pie";

</script>
<!DOCTYPE HTML>
<html>
<head>
<script>
window.onload = function() {

var chart = new CanvasJS.Chart("chartContainer", {
	animationEnabled: true,
	title: {
        text: Nome_Grafico
	},
	data: [{
		type: TipoGrafico,
		startAngle: 240,
		yValueFormatString: "##0.00\"%\"",
		indexLabel: "{label} {y}",
		dataPoints: [ 
			{y: 79.45, label: "Google"},
			{y: 7.31, label: "Bing"},
			{y: 7.06, label: "Baidu"},
			{y: 4.91, label: "Yahoo"},
			{y: 1.26, label: "Others"}
		]
	}]
});
chart.render();

}
</script>
</head>
<body>
	<div>
		<p>Página Inicial</p>
	</div>
<div id="chartContainer" style="height: 370px; width: 100%;"></div>
<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>